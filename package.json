{
  "name": "finn-style",
  "version": "0.0.0",
  "private": true,
  "description": "ESLint config for FINN.no",
  "scripts": {
    "docs": "node bin/generate-rule-docs.js packages/eslint-config-finn/index.js > rules.md",
    "eslint-find-rules": "cd packages/eslint-config-finn && npm run eslint-find-rules",
    "preversion": "npm prune && npm test",
    "postinstall": "npm run install:config && npm run install:config-react",
    "install:config": "cd packages/eslint-config-finn && npm prune && npm install",
    "install:config-react": "cd packages/eslint-config-finn-react && npm prune && npm install",
    "pretest": "npm link packages/eslint-config-finn",
    "test": "eslint . && node test/config-test.js && npm run test:finn && npm run test:finn-react",
    "posttest": "npm unlink packages/eslint-config-finn",
    "test:finn": "cd packages/eslint-config-finn && npm test",
    "test:finn-react": "cd packages/eslint-config-finn-react && npm test"
  },
  "keywords": [
    "coding-standard",
    "standards",
    "eslint",
    "eslintconfig",
    "lint",
    "style"
  ],
  "repository": "finn-no/eslint-config-finn",
  "bugs": {
    "url": "https://github.com/finn-no/eslint-config-finn/issues"
  },
  "devDependencies": {
    "async": "^2.0.1",
    "eslint": "^3.4.0",
    "eslint-find-rules": "^1.13.0",
    "marked": "^0.3.6"
  },
  "license": "MIT"
}
